generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")   // used by Prisma Client at runtime
  directUrl = env("DIRECT_URL")     // used by migrations and introspection
}


model TestItem {
  id        String   @id @default(uuid())
  name      String
  createdAt DateTime @default(now())
}

